
.introduce{
  height: 800rpx;
}
.introduce-view{
background: #ffffff;
display: -webkit-box;
display: -webkit-flex;
display: flex;
-webkit-box-align: center;
-webkit-align-items: center;
        align-items: center;
height: 110rpx;
margin: 20rpx 0;
padding: 0 20rpx;
}
.introduce-view view:nth-child(1){
  width: 38rpx; height: 38rpx;
padding-right: 20rpx;}
.introduce-view image{
  width: 38rpx !important; 
  height: 38rpx !important;
}
.introduce-view text{display: block; font-size: 30rpx;}



